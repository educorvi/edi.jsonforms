<?xml version="1.0"?>
<registry
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="edi.jsonforms">

<!-- -*- extra stuff goes here -*- -->
    <records interface="plone.base.interfaces.resources.IBundleRegistry" prefix="plone.bundles/css">
      <value key="enabled">True</value>
      <value key="csscompilation">++resource++edi.jsonforms/css/bootstrap/bootstrap-icons.min.css</value>
      <value key="depends">plone</value>
    </records>

    <records interface="plone.base.interfaces.resources.IBundleRegistry" prefix="plone.bundles/css">
      <value key="enabled">True</value>
      <value key="csscompilation">++resource++edi.jsonforms/css/vue-json-form.css</value>
      <value key="depends">plone</value>
    </records>

    <records interface="plone.base.interfaces.resources.IBundleRegistry" prefix="plone.bundles/jscript">
      <value key="enabled">True</value>
      <value key="jscompilation">++resource++edi.jsonforms/js/vue-json-form.umd.js</value>
      <value key="load_async">False</value>
      <value key="load_defer">True</value>
      <value key="depends">plone</value>
    </records>


</registry>
